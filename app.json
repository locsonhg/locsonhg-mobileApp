{
  "expo": {
    "name": "Locsonhg",
    "slug": "locsonhg",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
      "image": "./assets/logo.jpeg"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.locsonhg.movieapp"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.locsonhg.movieapp"
    },
    "web": {},
    "plugins": [
      "expo-asset",
      "expo-video"
    ]
  }
}
